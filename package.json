{
  "name": "js-scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "build": "npm run build-client && npm run build-server",
    "build-client": "npx webpack --mode=production",
    "build-server": "node ./scripts/build-container",
    "clean": "rm -rf client/dist",
    "dev-server": "docker-compose up --build",
    "dev-client": "npx webpack --watch --mode=development",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\" "
  },
